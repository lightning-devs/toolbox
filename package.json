{
  "name": "@lightning-devs/toolbox",
  "version": "0.0.0",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-push": "npm run test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "scripts": {
    "nx": "nx",
    "start": "nx serve",
    "init": "npm ci",
    "build": "nx affected:build --all",
    "ci:build": "nx affected:build --with-deps --base=origin/master~1",
    "test": "nx affected:test --all",
    "lint": "nx workspace-lint && nx lint",
    "clean": "rm -rf tmp && npm run clean:libs && rm -rf node_modules",
    "clean:libs": "lerna exec -- rm -rf dist",
    "commitmsg": "commitlint -E $GIT_PARAMS",
    "prepublish": "npm run test",
    "publish": "lerna publish from-git --contents dist --yes",
    "version": "lerna version --include-merged-tags --conventional-commits --yes",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help"
  },
  "private": true,
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.1",
    "@nrwl/eslint-plugin-nx": "9.5.1",
    "@nrwl/jest": "^9.5.1",
    "@nrwl/node": "^9.5.1",
    "@nrwl/web": "^9.5.1",
    "@nrwl/workspace": "9.5.1",
    "@types/jest": "^25.1.4",
    "@types/node": "~8.9.4",
    "@typescript-eslint/eslint-plugin": "2.19.2",
    "@typescript-eslint/parser": "2.19.2",
    "dotenv": "6.2.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.0.0",
    "husky": "^4.2.5",
    "jest": "25.2.3",
    "lerna": "^3.22.1",
    "prettier": "2.0.4",
    "ts-jest": "^26.1.1",
    "ts-node": "~7.0.0",
    "tslint": "~6.0.0",
    "typescript": "~3.8.3"
  },
  "dependencies": {
    "awaity": "^1.0.0",
    "lodash": "^4.17.19"
  }
}
